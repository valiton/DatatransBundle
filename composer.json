{
    "name": "valiton/datatrans-bundle",
    "description": "Symfony3 bundle for providing access to Datatrans payment. This is an extension to the JMSCorePaymentBundle.",
    "type": "symfony-bundle",
    "keywords": ["payment","datatrans"],
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Cludius",
            "email": "sven.cludius@valiton.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^2.4|^3.0",
        "jms/payment-core-bundle": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "monolog/monolog": "1.18"
    },
    "autoload": {
        "psr-0": { "Valiton\\Payment\\DatatransBundle": "" }
    },
    "target-dir": "Valiton/Payment/DatatransBundle"
}
