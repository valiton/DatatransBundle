{
    "name": "valiton/datatrans-bundle",
    "description": "Symfony bundle for providing access to Datatrans payment. This is an extension to the JMSCorePaymentBundle.",
    "type": "symfony-bundle",
    "keywords": ["payment","datatrans"],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Cludius",
            "email": "sven.cludius@valiton.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-simplexml": "*",
        "ext-xml": "*",
        "jms/payment-core-bundle": "~1.0",
        "psr/log": "^1.0",
        "symfony/browser-kit": "^2.8|^3.4|^4.0",
        "symfony/config": "^2.8|^3.4|^4.0",
        "symfony/dependency-injection": "^2.8|^3.4|^4.0",
        "symfony/http-foundation": "^2.8|^3.4|^4.0",
        "symfony/http-kernel": "^2.8|^3.4|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "monolog/monolog": "1.18"
    },
    "autoload": {
        "psr-0": { "Valiton\\Payment\\DatatransBundle": "" }
    },
    "target-dir": "Valiton/Payment/DatatransBundle"
}
